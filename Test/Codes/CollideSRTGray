#include <iostream>
#include <omp.h>
#include <unistd.h>
#include <cmath>
#include <eigen3/Eigen/Dense>

using namespace std;
using namespace Eigen;

const int Num = 4;

int main()
{
    double Porosity = 0.8;
    double Nu       = 0.075;
    double Fe       = 1.75/Porosity/sqrt(Porosity)/sqrt(150*Porosity);
    double Kp       = Porosity*Porosity*Porosity/(1-Porosity)/(1-Porosity)/150;
    double c0       = 0.5 + 0.25*Porosity*Nu/Kp;
    double c1       = 0.5 * Porosity * Fe / sqrt(Kp); 

    Vector3d v;
    v<<1,1,1;

    cout << Fe*v.norm()*Nu << endl;
    cout << Fe << "===" << Kp << "===" << c0 << "===" << c1 <<"==="<<endl;

    return 0;
}